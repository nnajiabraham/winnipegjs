{
  "name": "winnipeg-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"./src/**/*.js*\" --cache"
  },

  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "prettier": "^1.15.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier",
      "json"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "printWidth": 80,
          "singleQuote": true,
          "trailingComma": "all",
          "bracketSpacing": true,
          "arrowParens": "always"
        }
      ],
      "react/prop-types": 0,
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "props",
          "varsIgnorePattern": "props"
        }
      ]
    }
  }
}
